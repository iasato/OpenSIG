<?xml version="1.0" encoding="UTF-8"?>
<beanio xmlns="http://www.beanio.org/2011/01" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.beanio.org/2011/01 http://www.beanio.org/2011/01/mapping.xsd">
	<stream name="EFD" format="delimited">
		<writer>
			<property name="delimiter" value="|" />
		</writer>
		<record name="Dados" minOccurs="1" maxOccurs="1" class="br.com.opensig.fiscal.server.sped.blocoC.DadosC300">
            <field name="padrao" type="string" length="1" default="" />
            <field name="reg" type="string" length="4" trim="true" />
            <field name="cod_mod" type="string" length="2" trim="true" />
            <field name="ser" type="string" length="4" trim="true" />
            <field name="sub" type="string" length="3" trim="true" />
            <field name="num_doc_ini" type="int" length="6" />
            <field name="num_doc_fin" type="int" length="6" />
            <field name="dt_doc" type="date" format="ddMMyyyy" default="" />
            <field name="vl_doc" type="double" format="##0.00" default="" />
            <field name="vl_pis" type="double" format="##0.00" default="" />
            <field name="vl_cofins" type="double" format="##0.00" default="" />
            <field name="cod_cta" type="string" length="255" trim="true" />
            <field name="padrao" type="string" length="1" default="" />
		</record>
	</stream>
</beanio>